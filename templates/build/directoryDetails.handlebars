<div data-source-entry-id="{{dataSourceEntryId}}">
<div class="directory-detail-title {{#if has_thumbnail}}has-thumbnail{{/if}}">{{#if has_thumbnail}}<span class="thumbnail"><span class="helper"></span><span class="img-wrapper circular" style="background-image: url('{{thumbnail}}');"></span></span>{{/if}}{{title}}</div>
{{#each fields}}
    <div class="directory-detail-label">{{label}}</div>
    <div class="directory-detail-value">{{{value}}}</div>
{{/each}}
</div>
