<table class="table table-hover">
<thead>
<tr>
    <th style="width:25%">Field</th>
    <th class="text-center">Filter</th>
    <th class="text-center">Search</th>
    <th style="width:40%">Format</th>
</tr>
</thead>
<tbody>
{{#each this}}
    <tr>
        <td>{{this}}</td>
        <td>{{{filterCheckbox this}}}</td>
        <td>{{{searchCheckbox this}}}</td>
        <td>{{{typeSelector this}}}</td>
    </tr>
{{/each}}
</tbody>
</table>
